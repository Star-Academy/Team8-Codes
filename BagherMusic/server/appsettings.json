{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	},
	"AllowedHosts": "*",
	"SearchService": {
		"ElasticServerUri": "http://localhost:9200",
		"SearchOptions": {
			"ResultCountPerPage": 10,
			"AcceptableFuzziness": 3,
